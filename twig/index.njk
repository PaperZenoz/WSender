{% set page = { title: 'WSender' } %}
{% extends "./parts/main.njk" %}
{% block content %}


    {% set  _ = 'enter' %}
    <div class="container">
        <div class="{{ _ }}">
            <div class="logo">
                <strong>w</strong>
            </div>
            <h2 class="{{ _ }}__title">
                Вход в личный кабинет
            </h2>
            <div class="{{ _ }}__subtitle">
                <p>
                    Введите номер телефона аккаунта WhatsApp, к которому будет привязан личный кабинет
                </p>
            </div>

            <a href="#" class="btn btn--solid">
